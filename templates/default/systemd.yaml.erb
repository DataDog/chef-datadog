<%= JSON.parse(({'logs' => @logs }).to_json).to_yaml %>

instances:
  <% @instances.each do |i| -%>
  - unit_names:
        <% i['unit_names'].each do |u| -%>
      - <%= u %>
        <% end -%>
    <% if i.key?('substate_status_mapping') -%>
    substate_status_mapping:
      <% i['substate_status_mapping'].each do |s| -%>
      <% s.keys.each do |k| %>
      <%= k %>:
        <% s["#{k}"].each do |l,v| %>
        <%= l %>: <%= v %>
        <% end -%>
      <% end -%>
      <% end -%>
    <% end -%>
    <% if i.key?('tags') -%>
    tags:
      <% i['tags'].each do |t| -%>
    - <%= t %>
      <% end -%>
    <% end -%>
  <% end -%>

init_config:
# No init_config details needed
