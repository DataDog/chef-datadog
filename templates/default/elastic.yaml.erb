instances:
  <% @instances.each do |i| -%>
  - url: <%= i["url"] %>
    <% unless i["shard_level_metrics"].nil? %>
    shard_level_metrics: <%= i["shard_level_metrics"] %>
    <% end %>
    <% unless i["pshard_stats"].nil? %>
    pshard_stats: <%= i["pshard_stats"] %>
    <% end %>
    <% unless i["is_external"].nil? %>
    is_external: <%= i["is_external"] %>
    <% end %>
    <% unless i["username"].nil? %>
    username: <%= i["username"] %>
    <% end %>
    <% unless i["password"].nil? %>
    password: <%= i["password"] %>
    <% end %>
    <% unless i["timeout"].nil? %>
    timeout: <%= i["timeout"] %>
    <% end %>
    <% if i.key?('tags') -%>
    tags:
      <% i['tags'].each do |t| -%>
      - <%= t %>
      <% end -%>
    <% end -%>
  <% end %>

# Nothing to configure below
init_config:
