instances:
<% @instances.each do |i| -%>
  - url: <%= i['url'] %>
    <% unless i['ssl_verify'].nil? %>
    ssl_verify: <%= i['ssl_verify'] %>
    <% end %>
    <% unless i['ssl_cert'].nil? %>
    ssl_cert: <%= i['ssl_cert'] %>
    <% end %>
    <% unless i['ssl_key'].nil? %>
    ssl_key: <%= i['ssl_key'] %>
    <% end %>
    <% if i.key?('tags') -%>
    tags:
      <% i['tags'].each do |t| -%>
      - <%= t %>
      <% end -%>
    <% end -%>
<% end -%>

# Nothing to configure here
init_config:
