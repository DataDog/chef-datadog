instances:
<% @instances.each do |i| -%>
  - host: <%= i['host'] %>
    <% if i.key?('username') -%>
    username: <%= i['username'] %>
    <% end -%>
    <% if i.key?('password') -%>
    password: <%= i['password'] %>
    <% end -%>
    services:
      - <%= i['service'] %>
  <% if i.key?('tags') -%>
    tags:
    <% i['tags'].each do |t| -%>
    - <%= t %>
    <% end -%>
  <% end -%>
<% end -%>

init_config:
