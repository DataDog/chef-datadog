instances:
  <% @instances.each do |i| -%>
  - url:      <%= i['url'] %>
    <% if i.key?('username') && i.key?('password') -%>
    username: <%= i['username'] %>
    password: <%= i['password'] %>
    <% end -%>
    <% if i.key?('status_check') -%>
    status_check: True
    <% end -%>
  <% end -%>

# Nothing to configure below
init_config:
